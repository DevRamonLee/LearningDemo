<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".customview.CustomViewActivityDemo3">
    <!-- StyleTestView 验证属性的优先级
    直接在XML中定义 > style定义 > 由defStyleAttr定义的值 > defStyleRes指定的默认值 > 直接在Theme中指定的值 -->
    <ramon.lee.androidui.customview.view.PropertiesPriorityView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:attr_one="attr one in xml"
        style="@style/ThroughStyle"
        android:background="@color/blue"
        android:textColor="#FFFF00"
        android:text="@string/properties_priority"/>
</androidx.constraintlayout.widget.ConstraintLayout>
